//tutorial solution

#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
    int n,m,p=0; //n = size of the array    m = element to be searched
    cin>>n>>m;
    int*a =new int[n];
    for(int i=1;i<=n;i++)
    {
        cin>>a[i];
    }
    for(int i=1;i<=n;i++)
    {
        if(a[i]==m)
        {
            p = i;   
        }
    }
    if(p!=0)
      cout<< p <<endl;
    else
      cout<<"-1"<<endl;
}

